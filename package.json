{
  "name": "object-state-store",
  "version": "1.1.3",
  "description": "observable stateful object store",
  "moduleName": "ObjectStateStore",
  "jsnext:main": "dist/ost.es.js",
  "module": "dist/ost.es.js",
  "main": "dist/ost.js",
  "umd:main": "dist/ost.umd.js",
  "scripts": {
    "test": "ava test.js --verbose",
    "bump": "standard-version",
    "lint": "eslint src test",
    "rollup": "rollup -c",
    "clear": "rimraf dist",
    "build": "cross-env NODE_ENV=production yarn run clear && yarn run rollup && yarn run minify:cjs && yarn run docs && yarn run size",
    "build-": "cross-env NODE_ENV=production npm-run-all --silent clear -p rollup -p minify:* -s docs size",
    "watch": "rollup -c --watch",
    "minify:cjs": "uglifyjs $npm_package_main -cm toplevel -o $npm_package_main -p relative --in-source-map ${npm_package_main}.map --source-map ${npm_package_main}.map",
    "minify:umd": "uglifyjs $npm_package_umd_main -cm -o $npm_package_umd_main -p relative --in-source-map ${npm_package_umd_main}.map --source-map ${npm_package_umd_main}.map",
    "docs": "documentation readme src/index.js --section API -q",
    "size": "echo \"Gzipped Size: $(strip-json-comments --no-whitespace $npm_package_main | gzip-size | pretty-bytes)\"",
    "release": "yarn run build -s && yarn run bump && git push --follow-tags origin master && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soenkekluth/object-state-store.git"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "deep",
    "path",
    "object",
    "redux",
    "store",
    "state",
    "observable",
    "key",
    "value",
    "map",
    "emit",
    "event"
  ],
  "author": "SÃ¶nke Kluth <soenke.kluth@gmail.com> (http://soenkekluth.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/soenkekluth/object-state-store/issues"
  },
  "homepage": "https://github.com/soenkekluth/object-state-store#readme",
  "dependencies": {
    "is-plain-obj": "1.1.0",
    "microdispatch": "^1.2.0",
    "object-assign": "^4.1.1",
    "object-path": "^0.11.4"
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-tree-shaking": "1.0.1",
    "cross-env": "^5.0.1",
    "documentation": "^4.0.0-rc.1",
    "eslint": "^4.1.1",
    "gzip-size-cli": "^2.0.0",
    "npm-run-all": "^4.0.2",
    "pretty-bytes-cli": "^2.0.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-memory": "^2.0.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.0.0",
    "standard-version": "^4.2.0",
    "strip-json-comments-cli": "^1.0.1",
    "uglify-js": "^2.6.2"
  }
}
